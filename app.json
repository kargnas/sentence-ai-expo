{
  "expo": {
    "name": "Mandarin AI",
    "slug": "chinese-learning",
    "scheme": "mandarinai",
    "version": "1.2.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#cf4237"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "locales": {
      "ko": "./assets/locales/ko.json",
      "zh-CN": "./assets/locales/zh_cn.json",
      "zh-TW": "./assets/locales/zh_tw.json",
      "zh-HK": "./assets/locales/zh_tw.json"
    },
    "ios": {
      "buildNumber": "12",
      "supportsTablet": true,
      "bundleIdentifier": "as.kargn.chineselearning",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app uses audio libraries that may reference microphone APIs, but does not actually use the microphone for any functionality."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "as.kargn.chineselearning"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "21217879-3fd9-4910-b23c-d62a50bf41ac"
      }
    },
    "owner": "kargnas",
    "plugins": [
      "expo-localization",
      [
        "react-native-bottom-tabs",
        {
          "translucent": true
        }
      ],
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-secure-store",
      "expo-sqlite"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/21217879-3fd9-4910-b23c-d62a50bf41ac"
    }
  }
}
